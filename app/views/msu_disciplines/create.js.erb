var empty = $("#empty-message");
var status = "<%= j flash[:error] %>";

console.log(status);

if (status == "ok") {
    if (empty) {
        empty.remove();
    }

    $('#disciplineModal').modal('hide');
    $('#disciplines').append("<%= j render(:partial => '/msu_disciplines/discipline', :object => @discipline) %>");

    // window.location.reload()
}
